{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=src react-scripts start",
    "build": "NODE_PATH=src react-scripts build",
    "eject": "react-scripts eject",
    "lint": "tslint --project ./tsconfig.json 'src/**/*.{ts,tsx}'",
    "test": "yarn test:raw | tap-spec",
    "test:raw": "NODE_ENV=test NODE_PATH=src ts-node --project tsconfig.test.json -r esm -r @babel/register -r jsdom-global/register ./node_modules/.bin/tape 'src/**/*.test.{ts,tsx}'",
    "docker:build": "docker build -t dex-app .",
    "docker:run": "docker run -p 3000:80 --rm -it dex-app",
    "docker:test": "yarn docker:builder && yarn docker:builder:test",
    "docker:builder": "docker build --target=builder -t dex-app-builder .",
    "docker:builder:test": "docker run -it dex-app-builder yarn test"
  },
  "dependencies": {
    "0x.js": "^2.0.6",
    "@0x/subproviders": "^2.1.6",
    "@0x/utils": "^2.0.6",
    "@0x/web3-wrapper": "^3.1.6",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "@types/node": "^10.12.12",
    "@types/ramda": "^0.25.47",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/web3": "^1.0.13",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.1.0",
    "date-fns": "^1.29.0",
    "joi": "^14.3.0",
    "normalizr": "^3.3.0",
    "ramda": "^0.26.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-stockcharts": "^0.7.8",
    "react-table": "^6.8.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.2.1",
    "uuid": "^3.3.2",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@0x/migrations": "^2.2.0",
    "@babel/register": "^7.0.0",
    "chance": "^1.0.17",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "esm": "^3.0.84",
    "ganache-cli": "^6.2.3",
    "http-proxy-middleware": "^0.19.1",
    "jsdom-global": "^3.0.2",
    "mock-socket": "^8.0.5",
    "redux-mock-store": "^1.5.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build/*"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
